# 市场数据服务配置
TUSHARE_TOKEN=your_tushare_token_here
AKSHARE_ENABLED=true

# Kafka配置
KAFKA_BOOTSTRAP_SERVERS=localhost:9092
KAFKA_TOPIC_STOCK_REALTIME=stock_realtime
KAFKA_TOPIC_STOCK_DAILY=stock_daily
KAFKA_TOPIC_INDEX_DATA=index_data

# 数据收集配置
COLLECT_REALTIME_INTERVAL=60  # 秒
COLLECT_DAILY_TIME=18:00     # 每日收集时间
STOCK_CODES=000001.SZ,000002.SZ,600000.SH  # 默认股票代码

# 日志配置
LOG_LEVEL=INFO
LOG_FILE=logs/market_data_service.log

# 服务配置
SERVICE_NAME=quant-market-data-service
SERVICE_PORT=8888